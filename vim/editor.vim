colorscheme jellybeans
set nu
set tabstop=4
set softtabstop=4
set shiftwidth=4
set expandtab
set ai
set encoding=utf-8
set nowrap
set hlsearch
set incsearch                  " find as you type search
set smartindent
set backspace=2
set iskeyword+=-

filetype plugin on
syntax enable
syntax on

let g:indentLine_faster = 1
set colorcolumn=120
